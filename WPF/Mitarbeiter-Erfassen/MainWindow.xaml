                <ComboBoxItem>Werkzeugkasten</ComboBoxItem>
                <ComboBoxItem>Schreibutensilien</ComboBoxItem>
            </ComboBox>
            <Label>ausgegebene Betriebsmittel</Label>
            <ListBox Name="ansichtBetriebsmittel">
                
            </ListBox>
        </StackPanel>
    </Grid>
</Window>
